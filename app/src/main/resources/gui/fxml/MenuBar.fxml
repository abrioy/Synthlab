<?import javafx.scene.control.*?>
<MenuBar xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
         fx:controller="fr.synthlab.view.controller.MenuBarController"
         stylesheets="/gui/fxml/style/MenuBar.css">
    <Menu text="File">
        <MenuItem text="New"/>
        <MenuItem text="Open"/>
        <MenuItem text="Save"/>
        <MenuItem text="Save As"/>
        <SeparatorMenuItem/>
        <MenuItem text="Exit"/>
    </Menu>
    <Menu text="Edit">
        <MenuItem text="Copy"/>
        <MenuItem text="Cut"/>
        <MenuItem text="Paste"/>
    </Menu>
    <Menu text="Help">
        <MenuItem text="Search" disable="true"/>
        <MenuItem text="Online Manual" visible="false"/>
        <SeparatorMenuItem/>
        <MenuItem text="About"/>
    </Menu>
</MenuBar>