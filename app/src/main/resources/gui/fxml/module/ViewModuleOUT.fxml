<?xml version="1.0" encoding="UTF-8"?>

<?import fr.synthlab.view.component.Knob?>
<?import fr.synthlab.view.component.Plug?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Rectangle?>
<?import fr.synthlab.view.component.MuteButton?>
<Pane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8"
      prefHeight="198" prefWidth="250" minHeight="198" minWidth="250" maxHeight="198" maxWidth="250">

    <Knob fx:id="picker" label="Gain"
          diameter="80.0" layoutX="190.0" layoutY="100.0"
          min="-12" max="12"/>

    <MuteButton fx:id="muteButton"
            layoutX="175.0" layoutY="155.0">
    </MuteButton>

    <Rectangle fx:id="rectStereo" mouseTransparent="true"
               height="70.0" width="115.0" layoutX="5" layoutY="50.0"/>
    <Plug fx:id="inStereoL"
          layoutX="15.0" layoutY="70.0"
          label="Left" name="Left" type="input" />

    <Plug fx:id="inStereoR"
          layoutX="65.0" layoutY="70.0"
          label="Right" name="Right" type="input" />


    <Rectangle fx:id="rectMono" mouseTransparent="true"
               height="70.0" width="65.0" layoutX="5.0" layoutY="125.0"/>
    <Plug fx:id="inMono"
          layoutX="15.0" layoutY="145.0"
          label="Mono" name="in" type="input" />
</Pane>
