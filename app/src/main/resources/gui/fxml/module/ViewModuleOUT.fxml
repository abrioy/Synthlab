<?xml version="1.0" encoding="UTF-8"?>

<?import fr.synthlab.view.component.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.shape.Rectangle?>
<Pane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8"
      prefHeight="200" prefWidth="250" minHeight="200" minWidth="250" maxHeight="200" maxWidth="250">

    <Knob fx:id="picker"
          diameter="80.0" layoutX="190.0" layoutY="100.0"
          max="-12" min="12"/>

    <Button fx:id="muteButton"
            layoutX="165.0" layoutY="145.0">
        Mute
    </Button>

    <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#2197ff00"
               height="60.0" width="115.0" layoutX="5" layoutY="70.0"
               stroke="BLACK" strokeType="INSIDE"  mouseTransparent="true"/>
    <Plug fx:id="inStereoL"
          layoutX="15.0" layoutY="80.0"
          name="inLeft" type="input" />

    <Plug fx:id="inStereoR"
          layoutX="65.0" layoutY="80.0"
          name="inRight" type="input" />


    <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#2197ff00"
               height="60.0" width="65.0" layoutX="5.0" layoutY="135.0"
               stroke="BLACK" strokeType="INSIDE" mouseTransparent="true"/>
    <Plug fx:id="inMono"
          layoutX="15.0" layoutY="145.0"
          name="in" type="input" />
</Pane>
