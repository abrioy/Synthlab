<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<ScrollPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="fr.synthlab.view.controller.ToolboxController"
            stylesheets="/gui/fxml/style/Toolbox.css">
    <Accordion fx:id="toolbox">
        <expandedPane>
            <TitledPane id="Type" fx:id="accordian" animated="false" text="Type">
                <content>
                    <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                </content>
            </TitledPane>
        </expandedPane>
        <panes>
            <fx:reference source="accordian" />
            <TitledPane fx:id="properties" animated="false" text="Size and colors">
                <content>
                    <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                </content>
            </TitledPane>
        </panes>
    </Accordion>
</ScrollPane>
