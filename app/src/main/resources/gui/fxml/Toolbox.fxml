<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>

<ScrollPane stylesheets="/gui/fxml/style/Toolbox.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.synthlab.view.controller.ToolboxController">
   <content>
       <Accordion fx:id="toolbox">
           <panes>
               <TitledPane id="Type" fx:id="input" animated="false" text="Input">
                   <content>
                       <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                   </content>
               </TitledPane>
               <TitledPane fx:id="output" animated="false" text="Output">
                   <content>
                       <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                   </content>
               </TitledPane>
            <TitledPane fx:id="filter" animated="false" text="Filter">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
              </content>
            </TitledPane>
            </panes>
        </Accordion>
    </content>
</ScrollPane>
